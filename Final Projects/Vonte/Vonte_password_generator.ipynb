        "#Generating a random password\n",
        "#Find a generator command\n",
        "\n",
        "def password(length):\n",
        "    pw = str()\n",
        "    characters = \"DavonteHuzle123456789?\"\n",
        "    for i in range(length):\n",
        "      pw = pw + random.choice(characters)\n",
        "    print(pw)\n",
        "    return (pw)\n",
        "\n",
        "password(40)"
